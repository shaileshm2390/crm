            <%
            var isPageRestrict = false;
                var rolesAndRestrictPages = [{
                role: 'Admin',
                pageRestrict: []
            },
            {
                role: 'Dev',
                pageRestrict: ['/customer/[0-9]/edit',
                                            '/customer/create',
                                                '/user', 
                                                '/user/create', 
                                                '/user/[0-9]/edit',
                                                '/user/[0-9]',
                                                '/department',
                                                '/department/[0-9]/edit',
                                                '/department/create',
                                                '/department/[0-9]',
                                                '/customer/[0-9]/buyer/create',
                                                '/customer/[0-9]/buyer/[0-9]/edit',
                                            ]
            },
            {
                role: 'Agent',
                pageRestrict: ['/customer/[0-9]/edit',
                                            '/customer/create',
                                                '/user', 
                                                '/user/create', 
                                                '/user/[0-9]/edit',
                                                '/user/[0-9]',
                                                '/department',
                                                '/department/[0-9]/edit',
                                                '/department/create',
                                                '/department/[0-9]',                                             
                                            ]
            }];

            var userRole = rolesAndRestrictPages.filter(function(o) { return o['role'] == user.role; });
            if(userRole.length) {
                userRole = userRole[0];           
                if(userRole.pageRestrict.length ) {
                    isPageRestrict = new RegExp(userRole.pageRestrict.join("|", "i")).test(req.originalUrl);
                } else {
                    isPageRestrict = false;
                }
            }
            %>
             <% if (((typeof user == 'object' && user.id != null && !isPageRestrict) || req.originalUrl == "/signin" || req.originalUrl == "/")) { %>
